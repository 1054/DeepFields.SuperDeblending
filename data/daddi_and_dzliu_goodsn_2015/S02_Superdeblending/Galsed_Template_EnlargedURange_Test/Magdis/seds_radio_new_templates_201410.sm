check 
    # 
    #     macro read seds_radio_new_templates_201410.sm check
    # 
    data README_dliu read {ilist 2 ulist 3}
    # 
    device x11
    erase expand 1.5
    limits 0 6 -4 0
    ticksize -1 10 -1 10
    box
    xlabel wavelength [um]
    ylabel normalized flux
    xtcolors
    set clist = {skyblue blue steelblue cyan green darkgreen seagreen forestgreen olivedrab gold magenta orange brown violet plum}
    set cedex = 0
    do i=0,dimen(ulist)-1 {
        echo "sed_z"$(ilist[$i])"_U"$(ilist[$i])"_radio.txt"
        data "sed_z"$(ilist[$i])"_U"$(ilist[$i])"_radio.txt" read {w 1 f 2}
        expand 0.25
        ctype $(clist[cedex])
        set cedex=cedex+1
        ptype 1 3
        connect (lg(w)) (lg(f))
        expand 1.5
        ctype default
    }



