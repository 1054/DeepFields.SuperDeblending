goCubicInterp4
    # 
    # 
    # 
    set _gocuin4_a0 = $4 - $3 - $1 + $2
    set _gocuin4_a1 = $1 - $2 - _gocuin4_a0
    set _gocuin4_a2 = $3 - $1
    set _gocuin4_a3 = $2
    set _gocuin4_ff = _gocuin4_a0*mu*mu*mu + _gocuin4_a1*mu*mu + _gocuin4_a2*mu + _gocuin4_a3

