    set old_ulist = {2.2 3.3 4.9 6.1 9.7 12.1 14.5 18.0}
    do i=0,dimen(old_xlist)-1 {
        data sed_z$(old_xlist[$i])""_U$(old_xlist[$i])""_radio.txt
        read < old_w$(old_xlist[$i]) 1 old_f$(old_xlist[$i]) 2 >
    }
    set add_xlist = { 9 10 11 12 13 14}
    set add_ulist = {25 30 35 40 45 50}
    do i=0,dimen(add_xlist)-1 {
        data seds_radio_new_templates_201410.txt
        read < old_w$(add_xlist[$i]) 1 old_f$(add_xlist[$i]) $(2+$i) >
    }
    set old_xlist = old_xlist concat add_xlist
    set old_ulist = old_ulist concat add_ulist
    # <TODO> then interpolate ???
    set old_u0 = 3.3 set old_x0 = 2
    set old_u1 = 4.9 set old_x1 = 3
    set old_u2 = 6.1 set old_x2 = 4
    set old_u3 = 9.7 set old_x3 = 5
    set new_u1 = 5.5
    set new_f1 = goCubicInterp4(old_f2 old_f3 old_f4 old_f5 new_u1)
    device x11
