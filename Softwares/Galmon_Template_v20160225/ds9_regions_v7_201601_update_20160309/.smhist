quit
load rfile.sm
rfile SED_predictions_100.txt
load astroPhot.sm
set_goodsn_photometry_constants_100
set col100 = (exc100==0)? 'yellow' : 'brown'
set sig100 = 0.3
set rad100 = (xf100<sig100*5)? xf100/(sig100*5)*($rBeam_100/2) : ($rBeam_100/2)
define print_noheader 1
print "cat_100.ds9.reg" 'fk5\n' {}
print +"cat_100.ds9.reg" 'circle(%0.7f,%0.7f,%0.3f\") \# dash=%d color=%s\n' {_ra _de rad100 _id exc100 col100}
quit
