    # 
    # Check input
    if(!$?zMIN || !$?zMAX) {return}
    # 
    # DEFINE parameters
    define setDoHistogram 0
    #define hasReadData 0
    # 
    # Read Catalog
    #chdir "../catalog_daddi_dzliu_goodsn_2016/"
    #macro read CatalogReader.sm go
    #chdir "../Plot_Mstar_SFR/"
